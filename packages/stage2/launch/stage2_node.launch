<launch>
    <arg name="veh" doc="Name of vehicle. ex: megaman"/>
    <arg name="pkg_name" value="stage2"/>
    <arg name="node_name" default="stage2_node"/>
    <group ns="$(arg veh)">
        <node name="$(arg node_name)" pkg="$(arg pkg_name)" type="stage2_node.py" output="screen" required="true">
            <param name="veh" value="$(arg veh)" />
        </node>
    </group>
</launch>
